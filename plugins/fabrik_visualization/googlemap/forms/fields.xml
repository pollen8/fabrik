				label="COM_FABRIK_LIST_PREFILTERS_LABEL"
				type="fabrikmodalrepeat">

				<fields name="params">
					<fieldset
						modal="true"
						name="prefilters_modal"
						repeat="true">

						<field
							name="filter-join"
							type="list"
							default="OR"
							repeat="true"
							label="">
								<option value="AND">AND</option>
								<option value="OR">OR</option>
						</field>

						<!-- <field name="filter-fields" connection="googlemap_connection" 
