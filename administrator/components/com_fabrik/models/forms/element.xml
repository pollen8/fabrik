				description="COM_FABRIK_FIELD_FILTER_LENGTH_DESC"
				label="COM_FABRIK_FIELD_FILTER_LENGTH_LABEL" />
				
			<field name="filter_access"
				type="accesslevel"
				default="1"
				label="COM_FABRIK_FIELD_FILTER_ACCESS_LABEL" />
		
