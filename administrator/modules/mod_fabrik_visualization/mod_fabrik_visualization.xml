<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<extension client="administrator" method="upgrade" type="module" version="3">
	<name>Fabrik Visualization</name>
	<author>Rob Clayburn</author>
	<creationDate>March 2016</creationDate>
	<copyright>Copyright (C) 2005-2016 fabrikar.com - All rights reserved.</copyright>
	<license>GNU/GPL http://www.gnu.org/copyleft/gpl.html</license>
	<authorEmail>rob@pollen-8.co.uk</authorEmail>
	<authorUrl>www.fabrikar.com</authorUrl>
	<version>3.4.3</version>
	<description>This module shows a Fabrik visualization in Joomla's administration pages.</description>
	<files>
		<filename module="mod_fabrik_visualization">mod_fabrik_visualization.php</filename>
		<filename>index.html</filename>
		<filename>mod_fabrik_visualization.xml</filename>
	</files>
	<config>
		<fields name="params">
			<fieldset addfieldpath="/administrator/components/com_fabrik/models/fields" name="advanced">
				<field default="0" description="" label="Select Instance" name="id" type="visualizationlist"/>
			</fieldset>
		</fields>
	</config>
</extension>
